{
  "name": "NETMigrationHelper",
  "displayName": ".Net Migration",
  "description": "Helps with managing .NET migrations with a graphical interface.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.98.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "NETMigrationHelper.Setup",
        "title": "MigrationHelper: Create Config File"
      },
      {
        "command": "NETMigrationHelper.AddMigration",
        "title": "MigrationHelper: Add Migration .NET"
      },
      {
        "command": "NETMigrationHelper.RemoveMigration",
        "title": "MigrationHelper: Remove Migration .NET"
      },
      {
        "command": "NETMigrationHelper.UpdateDatabase",
        "title": "MigrationHelper: Update Database .NET"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.98.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "eslint": "^9.19.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  }
}